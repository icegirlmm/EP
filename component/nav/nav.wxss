/* component/nav/nav.wxss */
.nav{
  width: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  font-size: 30rpx;
  color: #fff;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size:36rpx;
  letter-spacing:2px;
  font-weight: bolder;
}
.nav .nav-back{
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
  z-index: 99;
  display: inline-block;
  line-height: 45rpx;
}
.nav .back{
  height: 30rpx;
  width: 20rpx;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}